{
  "manifest_version": 3,
  "name": "TruthLens: Fake News Detector",
  "version": "1.0",
  "description": "Analyzes the current page content for potential misinformation.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["http://localhost:5000/*"],
  "icons": {
    "16": "icons/icon16.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png"
    }
  }
}